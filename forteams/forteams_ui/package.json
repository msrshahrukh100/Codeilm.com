{
  "name": "forteams_ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.0.2",
    "@material-ui/icons": "^3.0.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "collect": "react-scripts build && npm run build-rename && npm run copy-buildfiles",
    "build-rename": "npm run build-rename-js && npm run build-rename-css && npm run build-rename-logo",
    "build-rename-logo": "renamer --regex --find '([^\\.])\\.[^\\.]+\\.(\\w+)' --replace '$1.$2' build/static/media/*",
    "build-rename-js": "renamer --regex --find 'main\\.[^\\.]+\\.js' --replace 'forteams.ui.js' build/static/js/*.js",
    "build-rename-css": "renamer --regex --find 'main\\.[^\\.]+\\.css' --replace 'forteams.ui.css' build/static/css/*.css",
    "copy-buildfiles": "npm run copy-build-js && npm run copy-build-css && npm run copy-build-logo",
    "copy-build-logo": "copyfiles -f 'build/static/media/*' '../static/forteams/img/'",
    "copy-build-js": "copyfiles -f 'build/static/js/*.js' '../static/forteams/js/'",
    "copy-build-css": "copyfiles -f 'build/static/css/*.css' '../static/forteams/css/'"
  },
  "devDependencies": {
    "copyfiles": "^2.1.0",
    "renamer": "^0.7.0"
  }
}
